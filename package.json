    "@types/body-parser": "^1.17.0",
    "@types/jsonwebtoken": "^8.3.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "3.5.1",
    "body-parser": "^1.18.2",
    "chalk": "^2.4.2",

    "@types/node": "^8.10.55",
    "@types/passport": "^1.0.1",
    "@types/passport-local": "^1.0.33",
    "concurrently": "^3.5.1",
    "jest": "^24.9.0",
    "sequelize-mock": "^0.10.2",
    "snyk": "^1.235.0",
    "ts-jest": "^24.1.0",
    "ts-node": "^3.3.0",
    "tsc-watch": "^1.0.8",
    "tslint": "^5.20.0"
